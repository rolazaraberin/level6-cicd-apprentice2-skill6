{
  "name": "level6-cicd-apprentice2-skill6",
  "version": "1.0.0",
  "engines": {
    "node": "18.14.0"
  },
  "scripts": {
    "start": "npm run prod-start",
    "build": "npm run prod-build",
    "test": "npm run prod-test",
    "deploy": "npm run github-push",
    "dev-start": "nodemon ./src/index.ts",
    "dev-build": "rm -rf build && npm run typescript-build && npm run minify",
    "dev-test": "jest --runInBand --config jest.config.development.ts",
    "dist-build": "npm run webpack-backend",
    "prod-start": "node ./dist/index.js",
    "prod-build": "",
    "prod-test": "jest --runInBand --detectOpenHandles --config jest.config.production.ts",
    "github-push": "git push github",
    "github-pull": "git pull github",
    "github-add": "git remote add github",
    "github-logout": "npm run github-info && git credential reject",
    "github-login": "npm run github-info && git credential fill",
    "github-info": "echo Type protocol=https host=github.com",
    "tables-knex": "knex migrate:latest --env cockroachdb",
    "seed-knex": "knex seed:run --env cockroachdb",
    "migration-knex": "npm run tables-knex && npm run seed-knex",
    "rollback-knex": "knex migrate:rollback --env cockroachdb",
    "tables-mysql": "knex migrate:latest --env mysql",
    "posttables-mysql": "knex seed:run --env mysql",
    "migration-mysql": "knex migrate:latest --env mysql",
    "postmigration-mysql": "knex seed:run --env mysql",
    "data-mysql": "knex seed:run --env mysql",
    "seed-mysql": "knex seed:run --env mysql",
    "rollback-mysql": "knex migrate:rollback --env mysql",
    "migration-elephantsql": "knex migrate:latest --env elephantsql",
    "postmigration-elephantsql": "knex seed:run --env elephantsql",
    "seed-elephantsql": "knex seed:run --env elephantsql",
    "rollback-elephantsql": "knex migrate:rollback --env elephantsql",
    "tables-typeorm": "typeorm-ts-node-commonjs schema:sync --dataSource data-source.ts",
    "seed-typeorm": "typeorm-ts-node-commonjs migration:run --dataSource data-source.ts",
    "migration-typeorm": "npm run tables-typeorm && npm run seed-typeorm",
    "rollback-typeorm": "typeorm-ts-node-commonjs migration:revert --dataSource data-source.ts",
    "migration": "npm run migration-knex && npm run migration-typeorm",
    "rollback": "npm run rollback-knex && npm run rollback-typeorm",
    "typescript-build": "tsc",
    "compile-minify": "webpack --config webpack.config.minify.js",
    "build-production": "npm run minify",
    "build-netlify": "netlify build",
    "build-start": "./build-start.sh",
    "minify": "node ./minify.js build",
    "webpack": "webpack",
    "watch": "jest --watchAll",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@netlify/functions": "^1.4.0",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.1.2",
    "better-sqlite3": "^7.6.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "mysql": "^2.18.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^3.1.1",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.7.16",
    "babel-loader": "^8.2.5",
    "cockroachdb": "^1.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^28.1.3",
    "netlify-cli": "^12.10.0",
    "terser": "^5.16.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
