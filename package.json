    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "mysql": "^2.18.1",
    "pg": "^8.8.0",
